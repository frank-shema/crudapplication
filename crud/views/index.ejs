    <div>
        <% if(users!='' ){%>

            <table border="0" cellspacing="0" cellpadding="0">
                <thead>
                    <tr class="table">
                        <th>ID</th>
                        <th>Image</th>
                        <th>Name</th>
                        <th>E-mail</th>
                        <th>Phone</th>
                        <th>action</th>
                    </tr>
                </thead>

                <tbody>

                    <%users.forEach((row,index)=>{%>
                        <tr class="body">
                            <td>
                                <%= index %>
                            </td>
                            <td><img src="<%=row.image %>" width="50" height="40" class="" alt=""></td>
                            <td>
                                <%=row.name%>
                            </td>
                            <td>
                                <%= row.email %>
                            </td>
                            <td>
                                <%= row.phone %>
                            </td>
                            <td>
                                <a href="/edit/<%= row._id %>"><svg xmlns="http://www.w3.org/2000/svg" height="48"
                                        viewBox="0 96 960 960" width="48">
                                        <path
                                            d="M793 395 667 270l39-40q19-19 46.5-19t49.5 20l31 31q22 21 19.5 48.5T831 357l-38 38Zm-41 41L245 943H120V817l506-505 126 124Z" />
                                    </svg></a>

                                <a href="/delete/<%= row._id%>"> <svg xmlns="http://www.w3.org/2000/svg" height="48"
                                        style="fill: red;" viewBox="0 96 960 960" width="48">
                                        <path
                                            d="M253 957q-36.188 0-64.094-26.912Q161 903.175 161 866V314h-58v-91h228v-47h297v47h228v91h-58v552q0 37.175-27.206 64.088Q743.588 957 706 957H253Zm104-169h74V390h-74v398Zm173 0h75V390h-75v398Z " />
                                    </svg></a>
                            </td>
                        </tr>
                        <%})%>
                </tbody>
            </table>
            <%} else{ %>
                <h1 style="color: #5d5d5a;">No users found in the database! </h1>
                <% }%>

    </div>
